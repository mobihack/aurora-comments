{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [
        "src": "/.*",
        "dest": "/index.js"
    }
],
"env": {
    "GITHUB_TOKEN": "@github_token",
    "RECAPTCHA_SECRET_TOKEN": "@recaptcha_secret"
}
}