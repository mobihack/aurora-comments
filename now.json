{
    "version": 2,
    "builds": [
        {
            "src": "index.js",
            "use": "@now/node-server"
        }
    ],
    "routes": [{
        "headers": {
            "Access-Control-Allow-Origin": "*",
            "Access-Control-Allow-Methods": "GET, POST",
            "Access-Control-Allow-Headers": "X-Requested-With, Content-Type, Accept"
        },
        "src": "/.*",
        "dest": "/index.js"
    }],
    "env": {
        "GITHUB_TOKEN": "@github_token",
        "RECAPTCHA_SECRET_TOKEN": "@recaptcha_secret"
    }
}